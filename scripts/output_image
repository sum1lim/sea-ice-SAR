#!/usr/bin/env python3
import argparse


def main(args):
    None


if __name__ == "__main__":
    parser = argparse.ArgumentParser()

    parser.add_argument(
        "--input",
        type=str,
        help="Input CSV",
    )
    parser.add_argument(
        "--label-col",
        type=int,
        help="Index of the label column (starting from 0)",
    )

    args = parser.parse_args()
    args = parser.parse_args()

    main(args)
