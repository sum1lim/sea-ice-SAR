#!/usr/bin/env python3
import argparse
import sys
from utils.data_processing import get_coordinates


def main(args):
    if args.tr_data == None or args.te_data == None:
        print("ERROR: Training or test data not provided", file=sys.stderr)


if __name__ == "__main__":
    parser = argparse.ArgumentParser()

    parser.add_argument(
        "--tr-data",
        type=str,
        help="Directory path where raw training data reside in tif format",
    )
    parser.add_argument(
        "--te-data",
        type=str,
        help="Directory path where raw test data reside in csv format",
    )

    args = parser.parse_args()
    main(args)
